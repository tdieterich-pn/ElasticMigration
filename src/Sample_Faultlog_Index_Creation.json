  "settings":{
    "number_of_shards":1,
    "number_of_replicas":0,
    "analysis":{
      "filter":{
        "search_service_ngram_filter":{
          "type":"ngram",
          "min_gram":"1",
          "max_gram":"255"
        }
      },
      "analyzer":{
        "search_service_analyzer":{
          "filter":[
            "standard",
            "lowercase",
            "search_service_ngram_filter"
          ],
          "char_filter":[
            "at_mapping",
            "hyphen_mapping"
          ],
          "type":"custom",
          "tokenizer":"standard"
        },
        "standard_with_char_map":{
          "filter":"lowercase",
          "char_filter":[
            "at_mapping",
            "hyphen_mapping"
          ],
          "tokenizer":"standard"
        },
        "case_insensitive_sort":{
          "filter":"lowercase",
          "tokenizer":"keyword"
        }
      },
      "char_filter":{
        "at_mapping":{
          "type":"mapping",
          "mappings":"@=>_"
        },
        "hyphen_mapping":{
          "type":"mapping",
          "mappings":"-=>_"
        }
      }
    }
  },
